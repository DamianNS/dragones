<div class="focus-ring focus-ring-warning" tabindex="0" id="tile-@X-@Y" style="height: @(Size)px; width: @(Size)px; background-color: grey;border: solid 1px red;"></div>
@code{
    [Parameter] public int X { get; set; }
    [Parameter] public int Y { get; set; }
    [Parameter] public int Size { get; set; }
}